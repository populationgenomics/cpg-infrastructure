{
  "taskGroups": [
    {
      "taskSpec": {
        "runnables": [
          {
            "container": {
              "imageUri": "australia-southeast1-docker.pkg.dev/cpg-common/images/autoclass-migration:latest"
            },
            "environment": {
              "variables": {
                "BUCKET": "$BUCKET",
                "BILLING_PROJECT": "$BILLING_PROJECT",
                "SLACK_WEBHOOK": "$SLACK_WEBHOOK"
              }
            },
            "timeout": "172800s"
          }
        ],
        "computeResource": {
          "cpuMilli": 16000,
          "memoryMib": 16384
        }
      }
    }
  ],
  "allocationPolicy": {
    "serviceAccount": {
      "email": "autoclass-migration@cpg-common.iam.gserviceaccount.com"
    },
    "instances": [
      {
        "policy": { "machineType": "e2-highcpu-16" }
      }
    ]
  },
  "logsPolicy": {
    "destination": "CLOUD_LOGGING"
  }
}
